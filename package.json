    "@angular/platform-server": "^14.2.2",
    "@angular/router": "^14.2.2",
    "@nguniversal/express-engine": "^14.2.0",
    "bootstrap": "^5.2.1",
    "express": "^4.15.2",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
